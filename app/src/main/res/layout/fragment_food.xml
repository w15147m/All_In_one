<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/imagesback"
    android:orientation="vertical"
    android:paddingTop="20dp"
    tools:context=".fooddata.food"
    tools:ignore="SpeakableTextPresentCheck">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="3dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="5dp">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:scaleType="fitXY"

            android:src="@drawable/num1"></ImageView>

    </androidx.cardview.widget.CardView>
    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Food Types"
        android:gravity="center"
        android:layout_marginTop="20dp"
        android:textSize="40dp"
        android:textColor="#0170C8"
        />
    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:padding="20dp"


        >

        <RadioButton
            android:id="@+id/vag"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/vegetable_1"
            android:padding="20dp"
            android:text="  Veg"
            android:textSize="15dp" />


        <RadioButton
            android:id="@+id/nonvage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:drawableStart="@drawable/nonvag_1_removebg_preview"
            android:padding="20dp"
            android:text=" non Veg"
            android:textSize="20dp" />

        <RadioButton
            android:id="@+id/drinks"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:drawableStart="@drawable/beverage"
            android:padding="20dp"
            android:text="drinks"

            android:textSize="20dp" />

    </RadioGroup>



</LinearLayout>


